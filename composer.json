{
  "name": "contao-thememanager/ctm-effects",
  "type": "symfony-bundle",
  "description": "Brings animations and effects to the ThemeManager components",
  "keywords": [
    "effects",
    "theme"
  ],
  "homepage": "https://www.oveleon.de/",
  "authors": [
    {
      "name": "Oveleon",
      "homepage": "https://oveleon.de/",
      "role": "Developer"
    },
    {
      "name": "Daniele Sciannimanica",
      "homepage": "https://github.com/doishub",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "contao/core-bundle": "^4.13 || 5.1.*",
    "contao-thememanager/core": "^2.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.3.1"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "ContaoThemeManager\\Effects\\": "src/"
    },
    "classmap": [
      "contao/"
    ],
    "exclude-from-classmap": [
      "contao/config/",
      "contao/dca/",
      "contao/languages/",
      "contao/templates/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    },
    "contao-manager-plugin": "ContaoThemeManager\\Effects\\ContaoManager\\Plugin"
  }
}
